    <SqlDataSource ConnectionString="Data Source=WS-NE-W7-64\SQL2008R2;Initial Catalog=LvsHonselmann;Persist Security Info=True;User ID=sa;Password=masterkey" ProviderName="System.Data.SqlClient" SelectCommand="Select a.*&#xD;&#xA;, CONVERT(varchar(10), b.[Date], 104) as Eingangsdatum&#xD;&#xA;,CASE WHEN IsVerpackt = 1&#xD;&#xA;      THEN 'verpackt' + char(13)&#xD;&#xA;      ELSE ''&#xD;&#xA;      END as Verpackt&#xD;&#xA;,CASE WHEN (SELECT COUNT (*)&#xD;&#xA;&#x9;    FROM Artikel a1&#xD;&#xA;&#x9;    INNER JOIN LEingang c1 on c1.ID=a1.LEingangTableID&#xD;&#xA;&#x9;    INNER JOIN SchadenZuweisung d1 ON d1.ArtikelID=a1.ID&#xD;&#xA;&#x9;    INNER JOIN Schaeden e1 ON e1.ID=d1.SchadenID&#xD;&#xA;&#x9;    WHERE c1.Auftraggeber=@AuftraggeberID) &gt; 0&#xD;&#xA;      THEN 'Schaden: ' + STUFF((SELECT ', ' + e2.Bezeichnung&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;FROM Artikel a2&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;INNER JOIN LEingang c1 on c1.ID=a2.LEingangTableID&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;LEFT OUTER JOIN SchadenZuweisung d2 ON d2.ArtikelID=a2.ID&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;LEFT OUTER JOIN Schaeden e2 ON e2.ID=d2.SchadenID&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;WHERE c1.Auftraggeber=@AuftraggeberID&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;FOR XML PATH (''))&#xD;&#xA;&#x9;&#x9;&#x9;  ,1,2,'') &#xD;&#xA;      ELSE ''&#xD;&#xA;      END as Schaden&#xD;&#xA;, STUFF(&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;(SELECT ', ' + CAST(e1.ID as nvarchar)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;FROM Artikel a1&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;INNER JOIN LEingang c1 ON c1.ID=a1.LEingangTableID&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;INNER JOIN SchadenZuweisung d1 ON d1.ArtikelID=a1.ID&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;INNER JOIN Schaeden e1 ON e1.ID=d1.SchadenID&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;WHERE a1.ID=a.ID &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;Order By e1.ID&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;FOR XML PATH ('')&#xD;&#xA;&#x9;&#x9;&#x9; )&#xD;&#xA;&#x9;&#x9;,1,2,'') as Anmerkung&#xD;&#xA;, CASE&#xD;&#xA;&#x9;WHEN (a.Laenge&gt;0)&#xD;&#xA;&#x9;&#x9;THEN CAST(a.Dicke as varchar (20))+' x '+ CAST(CAST(a.Breite as int) as varchar(20))+' x '+CAST(CAST(a.Laenge as int) as varchar(20))&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;ELSE CAST(a.Dicke as varchar (20))+' x '+ CAST(Cast(a.Breite as int) as varchar(20))&#xD;&#xA;&#x9;END as Abmessung&#xD;&#xA;FROM Artikel a&#xD;&#xA;INNER JOIN LEingang b ON b.ID=a.LEingangTableID&#xD;&#xA;LEFT JOIN LAusgang c ON c.ID =a.LAusgangTableID&#xD;&#xA;WHERE&#xD;&#xA;(&#xD;&#xA;&#x9;b.Auftraggeber=@AuftraggeberID&#xD;&#xA;&#x9;AND b.Date &lt;@Stichtag &#x9;&#xD;&#xA;&#x9;AND a.BKZ=1 &#xD;&#xA;&#x9;AND a.CheckArt=1 &#xD;&#xA;&#x9;AND b.[Check]=1&#xD;&#xA;)&#xD;&#xA;OR&#xD;&#xA;(&#xD;&#xA;&#x9;b.Auftraggeber=@AuftraggeberID&#xD;&#xA;&#x9;AND b.Date &lt;@Stichtag &#xD;&#xA;&#x9;AND (c.Datum &gt; @Stichtag AND c.Datum  &lt;GETDATE()) &#xD;&#xA;&#x9;AND a.BKZ=0 &#xD;&#xA;&#x9;AND a.CheckArt=1 &#xD;&#xA;&#x9;AND b.[Check]=1&#xD;&#xA;)&#xD;&#xA;&#xD;&#xA;ORDER BY&#xD;&#xA;CASE&#x9;WHEN @OrderBY = 1 THEN a.Dicke END ,  &#xD;&#xA;CASE&#x9;WHEN @OrderBY = 1 THEN a.Breite  END ,&#xD;&#xA;CASE&#x9;WHEN @OrderBY = 1 THEN a.Laenge END,&#xD;&#xA;CASE    WHEN @OrderBY = 2 THEN a.Produktionsnummer END,&#xD;&#xA;CASE&#x9;WHEN @OrderBY = 3 THEN a.ExMaterialNummer END,&#xD;&#xA;CASE&#x9;WHEN @OrderBY = 0 THEN a.LVS_ID END; &#xD;&#xA;" CommandTimeout="500" Name="sqlArtikelSource">
